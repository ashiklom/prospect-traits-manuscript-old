  journal = {Remote Sensing of Environment},
  author = {Cunliffe, Andrew M. and Brazier, Richard E. and Anderson, Karen},
  month = sep,
  year = {2016},
  keywords = {biomass,Biome: Grassland,Biophysical,Canopy height model,canopy structure,grassland,Notes,Notes: Skimmed,photogrammetry,Rangeland,RS: UAV,Semi-arid,SfM,Shrubland,UAS,UAV,Vegetation},
  pages = {129--143},
  file = {Cunliffe et al_2016_Ultra-fine grain.pdf:/home/ashiklom/MEGAsync/Zotero/Cunliffe et al_2016_Ultra-fine grain.pdf:application/pdf},
