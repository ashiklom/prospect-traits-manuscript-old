  number = {2},
  urldate = {2016-07-08},
  journal = {Ecological Applications},
  author = {Viskari, Toni and Hardiman, Brady and Desai, Ankur R. and Dietze, Michael C.},
  month = mar,
  year = {2015},
  keywords = {data assimilation,Ecosystem Demography model,ED2,leaf-out,Phenology,prediction,senescence,terrestrial ecosystem models,unread},
  pages = {546--558},
  file = {Viskari et al_2015_Model-data.pdf:/home/ashiklom/MEGAsync/Zotero/Viskari et al_2015_Model-data.pdf:application/pdf}
}

@article{wu_convergence_2016,
  title = {Convergence in Relationships between Leaf Traits, Spectra and Age across Diverse Canopy Environments and Two Contrasting Tropical Forests},
  issn = {1469-8137},
  doi = {10.1111/nph.14051},
  abstract = {* Leaf age structures the phenology and development of plants, as well as the evolution of leaf traits over life histories. However, a general method for efficiently estimating leaf age across forests and canopy environments is lacking.

* Here, we explored the potential for a statistical model, previously developed for Peruvian sunlit leaves, to consistently predict leaf ages from leaf reflectance spectra across two contrasting forests in Peru and Brazil and across diverse canopy environments.

* The model performed well for independent Brazilian sunlit and shade canopy leaves (R2~=~0.75\textendash{}0.78), suggesting that canopy leaves (and their associated spectra) follow constrained developmental trajectories even in contrasting forests. The model did not perform as well for mid-canopy and understory leaves (R2~=~0.27\textendash{}0.29), because leaves in different environments have distinct traits and trait developmental trajectories. When we accounted for distinct environment\textendash{}trait linkages \textendash{} either by explicitly including traits and environments in the model, or, even better, by re-parameterizing the spectra-only model to implicitly capture distinct trait-trajectories in different environments \textendash{} we achieved a more general model that well-predicted leaf age across forests and environments (R2~=~0.79).

* Fundamental rules, linked to leaf environments, constrain the development of leaf traits~and allow for general prediction of leaf age from spectra across species, sites and canopy environments.},
  language = {en},
