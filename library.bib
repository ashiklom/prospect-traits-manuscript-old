  keywords = {Bayesian,ecoinformatics,ecological forecast,Ecophysiology,Ecosystem Demography model,ecosystem model,meta-analysis,PEcAn,plant functional traits,sensitivity analysis,unread,variance decomposition},
  pages = {133--154},
