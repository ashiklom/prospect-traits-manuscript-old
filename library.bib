  month = dec,
  year = {2016},
  keywords = {Australia,Biome: Agricultural,Biome: Savanna,Climatology,Ecosystems,EVI,Horticulture,MODIS,Notes,Notes: Skimmed,photosynthesis,RS: Satellite,Seasons,Solar radiation,Summer,vegetation index},
  pages = {e0168168},
  file = {Baldi et al_2016_Vegetation.pdf:/home/ashiklom/MEGAsync/Zotero/Baldi et al_2016_Vegetation.pdf:application/pdf},
