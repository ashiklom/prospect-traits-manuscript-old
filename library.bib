  journal = {Functional Ecology},
  author = {Read, Quentin D. and Moorhead, Leigh C. and Swenson, Nathan G. and Bailey, Joseph K. and Sanders, Nathan J.},
  month = feb,
  year = {2014},
  keywords = {community structure,elevation gradient,environmental filtering,intraspecific variation,leaf mass per unit area (LMA),leaf nitrogen,literature review,meta-analysis,Notes,Notes: Skimmed,phenotypic plasticity,plant functional traits,Plants},
  pages = {37--45},
  file = {Read et al_2014_Convergent effects.pdf:/home/ashiklom/MEGAsync/Zotero/Read et al_2014_Convergent effects.pdf:application/pdf},
  groups = {NP traits,NP traits}
