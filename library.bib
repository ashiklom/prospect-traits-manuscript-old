  month = jan,
  year = {2016},
  keywords = {kernel density estimation,Plant functional types,plant strategies,plant traits,trade-offs,trait variation},
  pages = {563--575},
  file = {Verheijen et al_2016_Variation in trait.pdf:/home/ashiklom/MEGAsync/Zotero/Verheijen et al_2016_Variation in trait.pdf:application/pdf},
  groups = {NP traits,PROSPECT traits,NP traits,PROSPECT traits}
}

