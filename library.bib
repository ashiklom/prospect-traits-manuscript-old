  journal = {Proceedings of the National Academy of Sciences},
  author = {Asner, Gregory P. and Knapp, David E. and Anderson, Christopher B. and Martin, Roberta E. and Vaughn, Nicholas},
  month = dec,
  year = {2016},
  keywords = {Amazon basin,functional biogeography,Leaf traits,plant traits,Reading list,Tropical forests},
  pages = {E4043--E4051},
  file = {Asner et al_2016_Large-scale.pdf:/home/ashiklom/MEGAsync/Zotero/Asner et al_2016_Large-scale.pdf:application/pdf},
