  journal = {Remote Sensing},
  author = {Sparks, Aaron M. and Kolden, Crystal A. and Talhelm, Alan F. and Smith, Alistair M. S. and Apostol, Kent G. and Johnson, Daniel M. and Boschetti, Luigi},
