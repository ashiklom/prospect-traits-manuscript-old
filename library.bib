  month = oct,
  year = {2016},
  keywords = {Biome: Agricultural,forecasting corn yields,high-resolution satellite imagery,Notes,Notes: Skimmed,RS: High-resolution/Commercial,RS: Satellite,spatial econometric,vegetation index,within-field variability,Yield estimation},
  pages = {848},
  file = {Peralta et al_2016_Mid-Season.pdf:/home/ashiklom/MEGAsync/Zotero/Peralta et al_2016_Mid-Season.pdf:application/pdf},
