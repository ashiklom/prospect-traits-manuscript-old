  keywords = {conifer,field spectroscopy,leaf optical properties,Notes,Notes: Skimmed,Radiative transfer model,RTM inversion,RTM: LIBERTY},
  pages = {50--60},
  file = {Dawson et al_1998_LIBERTY—Modeling.pdf:/home/ashiklom/MEGAsync/Zotero/Dawson et al_1998_LIBERTY—Modeling.pdf:application/pdf},
