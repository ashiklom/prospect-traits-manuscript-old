  keywords = {field spectroscopy,foliar traits,Herbaceous species,leaf spectroscopy,Notes,Notes: Skimmed,Partial least squares regression (PLSR),plant functional traits,small width leaves},
  pages = {706--719},
  file = {Roelofsen et al_2014_Predicting leaf.pdf:/home/ashiklom/MEGAsync/Zotero/Roelofsen et al_2014_Predicting leaf.pdf:application/pdf},
