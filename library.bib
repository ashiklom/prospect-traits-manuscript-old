  author = {Magney, Troy S. and Eusden, Spencer A. and Eitel, Jan U. H. and Logan, Barry A. and Jiang, Jingjue and Vierling, Lee A.},
  month = jan,
  year = {2014},
  keywords = {Carotenoids,Experiment: Light,green laser return intensity (GLRI),Leaf pigments,Leaf pigments,LiDAR,LiDAR: terrestrial,light detection and ranging (LiDAR),non-photochemical quenching (NPQ),Notes,Notes: Skimmed,photochemical reflectance index (PRI),Remote sensing,terrestrial laser scanning (TLS),vegetation index,xanthophyll cycle},
  pages = {344--356},
  file = {Magney et al_2014_Assessing leaf.pdf:/home/ashiklom/MEGAsync/Zotero/Magney et al_2014_Assessing leaf.pdf:application/pdf},
