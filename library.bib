  author = {Sakschewski, Boris and {von Bloh}, Werner and Boit, Alice and Rammig, Anja and Kattge, Jens and Poorter, Lourens and Pe{\~n}uelas, Josep and Thonicke, Kirsten},
  month = jul,
  year = {2015},
  keywords = {Biome: Tropical,ecosystem model,functional diversity,gap model,individual-based model,leaf economic spectrum,Location: Amazon,Remote sensing,trade-off,trait variability},
  pages = {2711--2725},
  file = {Sakschewski et al_2015_Leaf and stem.pdf:/home/ashiklom/MEGAsync/Zotero/Sakschewski et al_2015_Leaf and stem.pdf:application/pdf},
  groups = {NP traits,Used,NP traits,Used,Used}
}

