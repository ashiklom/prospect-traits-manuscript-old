  timestamp = {2017-01-09T14:23:14Z},
  urldate = {2017-01-09},
  journal = {Global Change Biology},
  author = {Yang, Hualei and Yang, Xi and Zhang, Yongguang and Heskel, Mary A. and Lu, Xiaoliang and Munger, J. William and Sun, Shucun and Tang, Jianwu},
  month = dec,
  year = {2016},
  keywords = {carbon cycle,Chlorophyll,gross primary production,Leaf area index (LAI),Notes,Notes: Skimmed,photosynthesis,solar-induced fluorescence,vegetation index,vegetation indices},
  pages = {n/a--n/a},
  file = {Yang et al_2016_Chlorophyll.pdf:/home/ashiklom/MEGAsync/Zotero/Yang et al_2016_Chlorophyll.pdf:application/pdf},
  groups = {PROSPECT traits,Landsat inversion,INTERFACE RS group,PROSPECT calibration,PROSPECT traits,Landsat inversion,INTERFACE RS group,PROSPECT calibration}
