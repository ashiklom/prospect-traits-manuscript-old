  journal = {Journal of Geophysical Research: Atmospheres},
  author = {Pinty, B. and Gobron, N. and Widlowski, J.-L. and Lavergne, T. and Verstraete, M. M.},
  month = nov,
  year = {2004},
  keywords = {Biosphere/atmosphere interactions,Cloud physics and chemistry,inversion,Radiative transfer model,Remote sensing,RTM inversion,unread,vegetation canopy},
  pages = {D21205},
  file = {Pinty et al_2004_Synergy between 1-D.pdf:/home/ashiklom/MEGAsync/Zotero/Pinty et al_2004_Synergy between 1-D.pdf:application/pdf}
