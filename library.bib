  keywords = {Biodiversity,Biome: Wetland/Marsh,conservation,Ecology,Habitat,lakes,Notes,Notes: Skimmed,object-based image analysis,Remote sensing,spatial autocorrelation,waterbird,Wetlands,wildlife},
