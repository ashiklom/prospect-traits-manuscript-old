  month = feb,
  year = {2004},
  keywords = {Biodiversity,detrended correspondence analysis,fire regime,foliar traits,leaf life span,Notes,Notes: Skimmed,phylogenetic lineage,plant functional traits,Quercus,Soil moisture,soil nutrients},
  pages = {635--662},
  file = {Cavender-Bares et al_2004_Multiple Trait.pdf:/home/ashiklom/MEGAsync/Zotero/Cavender-Bares et al_2004_Multiple Trait.pdf:application/pdf},
