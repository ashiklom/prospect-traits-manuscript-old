  month = jan,
  year = {2017},
  keywords = {bioenergy crops,database,ecoinformatics,Ecosystem services,meta-analysis,Notes,Notes: Skimmed,open access,plant functional traits,plant traits,yields},
  pages = {n/a--n/a},
