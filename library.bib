  keywords = {Biogeochemistry,biomass,carbon cycle,Ecosystem ecology,Forest ecology,Notes,Notes: Skimmed,Plant Ecology},
  pages = {90--93},
  file = {Stephenson et al_2014_Rate of tree carbon.pdf:/home/ashiklom/MEGAsync/Zotero/Stephenson et al_2014_Rate of tree carbon.pdf:application/pdf}
