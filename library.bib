  month = mar,
  year = {2016},
  keywords = {Biodiversity,evolution,field spectroscopy,genetic variation,hierarchical organization of plant diversity,Hyperspectral,Hyperspectral data,Notes,Notes: Skimmed,optical classification,Partial least squares regression (PLSR),phylogenetic signal,plant functional traits,PLS-DA,populations,Remote sensing,tree of life},
  pages = {221},
