  month = sep,
  year = {2009},
  keywords = {Canopy spectral and directional reflectance,Notes,Notes: read in full,PROSAIL,PROSPECT,Radiative transfer model,radiative transfer models,RTM: PROSPECT,RTM: SAIL,Vegetation},
