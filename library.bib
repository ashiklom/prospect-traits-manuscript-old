  month = may,
  year = {2016},
  keywords = {climate change,deciduous trees,ecohydrology,laser spectroscopy,plant physiology,plant–water relations,root uptake,soil water,stable isotopes,unread},
  pages = {839--849},
  file = {Volkmann et al_2016_High-resolution.pdf:/home/ashiklom/MEGAsync/Zotero/Volkmann et al_2016_High-resolution.pdf:application/pdf}
