@article{grant_diffuse_1987,
  title = {Diffuse and Specular Characteristics of Leaf Reflectance},
  volume = {22},
  issn = {0034-4257},
  doi = {10.1016/0034-4257(87)90064-2},
  abstract = {Leaves are neither purely diffuse nor purely specular reflectors. Instead, they have both diffuse and specular characteristics. The diffuse, Lambertian character of leaf reflectance emanates primarily from the interior of the leaf through multiple scattering, although there may be a small contribution of scattering from rough elements at the leaf surface. The specular and non-Lambertian character of leaf reflectance arises at the surface of the leaf. In this paper, the evolution of our current understanding of the mechanisms of leaf reflectance is reviewed. The use of measurements of polarized reflectance to separate leaf reflectance into diffuse and specular components is discussed. A section on the factors influencing leaf reflectance-leaf structure and physiological disturbances-is included along with discussion on the manner in which these influences are manifest.},
