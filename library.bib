  month = jun,
  year = {2016},
  keywords = {Copernicus,forest health,in-situ sampling,literature review,Monitoring,Notes,Notes: Skimmed,Remote sensing,RS: Applications,RS: Satellite,sensor networks,sentinel satellites,standardization},
  pages = {471},
  file = {Pause et al_2016_In Situ-Remote.pdf:/home/ashiklom/MEGAsync/Zotero/Pause et al_2016_In Situ-Remote.pdf:application/pdf},
