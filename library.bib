  keywords = {Earth Resources Science and Observation Center,Goddard Space Flight Center,Landsat Data Continuity Mission,National Aeronautics and Space Administration,Operational Land Imager,Thermal Infrared Sensor,United States Geological Survey},
  pages = {11--21},
