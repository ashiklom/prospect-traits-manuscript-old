  editor = {Goldstein, Guillermo and Santiago, Louis S.},
  year = {2016},
  keywords = {Biome: Tropical,chlorophyll,climate change,Electron transport rate,Hydraulic architecture,Leaf pigments,Leaf pigments,Photoinhibition,photosynthesis,photosynthesis,plant physiology,plant physiology,Plasticity,plasticity,shade tolerance,Shade tolerant trees,Tree Biology},
  pages = {319--336},
