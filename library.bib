  urldate = {2016-05-16},
  journal = {Annals of Botany},
  author = {Kitajima, Kaoru and Cordero, Roberto A. and Wright, S. Joseph},
  month = jan,
  year = {2013},
  keywords = {Biome: Tropical,Fracture toughness,herbivory,leaf density,leaf life span,leaf toughness,light environment,ontogenetic changes,phenotypic plasticity,seedling survival,stem density,tropical lianas,tropical trees,unread},
  pages = {685--699},
  file = {Kitajima et al_2013_Leaf life span.pdf:/home/ashiklom/MEGAsync/Zotero/Kitajima et al_2013_Leaf life span.pdf:application/pdf},
