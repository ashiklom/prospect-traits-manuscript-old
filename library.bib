  author = {Ali, Abebe Mohammed and Skidmore, Andrew K. and Darvishzadeh, Roshanak and {van Duren}, Iris and Holzwarth, Stefanie and Mueller, Joerg},
  month = dec,
  year = {2016},
  keywords = {continuous wavelet analysis,INFORM,Inversion: Wavelets,LDMC,Leaf traits,Notes,Notes: Skimmed,plant functional traits,RS: Imaging spectroscopy,RTM: INFORM,SLA},
  pages = {68--80},
  file = {Ali et al_2016_Retrieval of forest.pdf:/home/ashiklom/MEGAsync/Zotero/Ali et al_2016_Retrieval of forest.pdf:application/pdf},
