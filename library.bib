  journal = {Global Change Biology},
  author = {Asner, Gregory P. and Martin, Roberta E.},
  month = jun,
  year = {2016},
  keywords = {biogeography,chemical phylogeny,elevation gradient,functional biogeography,functional traits,Leaf traits,Notes,Notes: Skimmed,plant functional traits,plant traits,soil fertility},
  pages = {2216--2227},
  file = {Asner_Martin_2016_Convergent.pdf:/home/ashiklom/MEGAsync/Zotero/Asner_Martin_2016_Convergent.pdf:application/pdf},
  groups = {NP traits,PROSPECT traits,Landsat inversion,PROSPECT calibration,EDR,NP traits,PROSPECT traits,Landsat inversion,PROSPECT calibration,EDR}
