  number = {7},
  urldate = {2016-08-16},
  journal = {Oikos},
  author = {Read, Quentin D. and Hoban, Sean M. and Eppinga, Maarten B. and Schweitzer, Jennifer A. and Bailey, Joseph K.},
  month = jul,
  year = {2016},
  keywords = {Biodiversity,Hierarchical models,Notes: Skimmed,plant functional traits},
  pages = {895--904},
  file = {Read et al_2016_Accounting for the.pdf:/home/ashiklom/MEGAsync/Zotero/Read et al_2016_Accounting for the.pdf:application/pdf},
  groups = {NP traits,NP traits}
