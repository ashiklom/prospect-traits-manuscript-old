  journal = {Environmental Pollution},
  author = {Pakeman, Robin J. and Alexander, Jim and Brooker, Rob and Cummins, Roger and Fielding, Debbie and Gore, Sarah and Hewison, Richard and Mitchell, Ruth and Moore, Emily and Orford, Katy and Pemberton, Clare and Trinder, Clare and Lewis, Rob},
  month = may,
