  journal = {Ecological Complexity},
  author = {Homolov{\'a}, Lucie and Malenovsk{\'y}, Zbyn{\v e}k and Clevers, Jan G. P. W. and Garc{\'\i}a-Santos, Glenda and Schaepman, Michael E.},
  month = sep,
