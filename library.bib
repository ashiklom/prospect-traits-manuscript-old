  keywords = {Biogeochemistry,environmental gradients,Fortuna Forest Reserve,nutrient limitation,nutrient limitation,phosphorus cycling,plant functional traits,stoichiometry,unread,wood density},
  pages = {n/a--n/a},
  file = {Heineman et al_2016_Variation in wood.pdf:/home/ashiklom/MEGAsync/Zotero/Heineman et al_2016_Variation in wood.pdf:application/pdf}
