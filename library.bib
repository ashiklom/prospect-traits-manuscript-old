  journal = {Oikos},
  author = {Fornoff, Felix and Klein, Alexandra-Maria and Hartig, Florian and Benadi, Gita and Venjakob, Christine and Schaefer, H. Martin and Ebeling, Anne},
  month = nov,
  year = {2016},
  keywords = {ecosystem service,functional diversity,Notes,Notes: Skimmed,plant functional traits,pollination},
  pages = {n/a--n/a},
  file = {Fornoff et al_2016_Functional flower.pdf:/home/ashiklom/MEGAsync/Zotero/Fornoff et al_2016_Functional flower.pdf:application/pdf},
