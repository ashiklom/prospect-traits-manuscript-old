  journal = {Oecologia},
  author = {Schaller, J{\"o}rg and Roscher, Christiane and Hillebrand, Helmut and Weigelt, Alexandra and Oelmann, Yvonne and Wilcke, Wolfgang and Ebeling, Anne and Weisser, Wolfgang W.},
  month = may,
  year = {2016},
  keywords = {Biodiversity,Biogeochemistry,Calcium,Ecology,Grasses,Hydrology/Water Resources,Jena Experiment,Legumes,nutrient cycling,Plant diversity,Plant Sciences,Silicon},
  pages = {1--10},
