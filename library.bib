  keywords = {canopy modeling,Eucalyptus delegatensis,gas exchange,scaling,stomatal conductance,transpiration,unread},
  pages = {1687--1699},
  file = {Medlyn et al_2007_Linking leaf and.pdf:/home/ashiklom/MEGAsync/Zotero/Medlyn et al_2007_Linking leaf and.pdf:application/pdf},
