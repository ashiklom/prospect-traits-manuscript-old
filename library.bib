  number = {5},
  urldate = {2016-05-20},
  journal = {Journal of Ecology},
  author = {Jung, Vincent and Violle, Cyrille and Mondy, C{\'e}dric and Hoffmann, Lucien and Muller, Serge},
  month = sep,
  year = {2010},
  keywords = {community null model,determinants of plant community diversity and structure,flood meadow,gradient analysis,habitat filtering,niche differentiation,plant functional traits,species coexistence,trait plasticity},
  pages = {1134--1140},
  file = {Jung et al_2010_Intraspecific.pdf:/home/ashiklom/MEGAsync/Zotero/Jung et al_2010_Intraspecific.pdf:application/pdf},
  groups = {NP traits,Used,NP traits,Used,Used}
