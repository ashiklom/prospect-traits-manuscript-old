  month = dec,
  year = {2016},
  keywords = {Climate change adaptation,Drought impacts,Ecosystem services,Pinus ponderosa,Pinus resinosa,Semi-arid forests,Temperate forests,Thinning,Tree population density,unread},
  pages = {n/a--n/a},
