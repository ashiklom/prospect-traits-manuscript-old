  author = {Baret, F. and Houl{\`e}s, V. and Gu{\'e}rif, M.},
  month = mar,
  year = {2007},
  keywords = {Chlorophyll,data assimilation,functioning model,inversion,Leaf area index (LAI),Nitrogen,Notes,Notes: Skimmed,precision farming,RTM inversion},
  pages = {869--880},
  file = {Baret et al_2007_Quantification of.pdf:/home/ashiklom/MEGAsync/Zotero/Baret et al_2007_Quantification of.pdf:application/pdf},
  groups = {Landsat inversion,INTERFACE RS group,EDR,Landsat inversion,INTERFACE RS group,EDR},
