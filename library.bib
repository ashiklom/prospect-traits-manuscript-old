  keywords = {Biogeochemistry,Eutrophication,Machair,Nitrogen deposition,Sand dune,Scotland,Vegetation change},
  pages = {337--347},
  file = {Pakeman et al_2016_Long-term impacts.pdf:/home/ashiklom/MEGAsync/Zotero/Pakeman et al_2016_Long-term impacts.pdf:application/pdf}
