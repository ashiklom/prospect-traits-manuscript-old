@article{serbin_spectroscopic_2014,
  title = {Spectroscopic Determination of Leaf Morphological and Biochemical Traits for Northern Temperate and Boreal Tree Species},
  volume = {24},
  issn = {1939-5582},
  doi = {10.1890/13-2110.1},
  abstract = {The morphological and biochemical properties of plant canopies are strong predictors of photosynthetic capacity and nutrient cycling. Remote sensing research at the leaf and canopy scales has demonstrated the ability to characterize the biochemical status of vegetation canopies using reflectance spectroscopy, including at the leaf level and canopy level from air- and spaceborne imaging spectrometers. We developed a set of accurate and precise spectroscopic calibrations for the determination of leaf chemistry (contents of nitrogen, carbon, and fiber constituents), morphology (leaf mass per area, Marea), and isotopic composition ($\delta$15N) of temperate and boreal tree species using spectra of dried and ground leaf material. The data set consisted of leaves from both broadleaf and needle-leaf conifer species and displayed a wide range in values, determined with standard analytical approaches: 0.7\textendash{}4.4\% for nitrogen (Nmass), 42\textendash{}54\% for carbon (Cmass), 17\textendash{}58\% for fiber (acid-digestible fiber, ADF), 7\textendash{}44\% for lignin (acid-digestible lignin, ADL), 3\textendash{}31\% for cellulose, 17\textendash{}265 g/m2 for Marea, and -9.4\textperthousand{} to 0.8\textperthousand{} for $\delta$15N. The calibrations were developed using a partial least-squares regression (PLSR) modeling approach combined with a novel uncertainty analysis. Our PLSR models yielded model calibration (independent validation shown in parentheses) R2 and the root mean square error (RMSE) values, respectively, of 0.98 (0.97) and 0.10\% (0.13\%) for Nmass, R2 = 0.77 (0.73) and RMSE = 0.88\% (0.95\%) for Cmass, R2 = 0.89 (0.84) and RMSE = 2.8\% (3.4\%) for ADF, R2 = 0.77 (0.69) and RMSE = 2.4\% (3.9\%) for ADL, R2 = 0.77 (0.72) and RMSE = 1.4\% (1.9\%) for leaf cellulose, R2 = 0.62 (0.60) and RMSE = 0.91\textperthousand{} (1.5\textperthousand) for $\delta$15N, and R2 = 0.88 (0.87) with RMSE = 17.2 g/m2 (22.8 g/m2) for Marea. This study demonstrates the potential for rapid and accurate estimation of key foliar traits of forest canopies that are important for ecological research and modeling activities, with a single calibration equation valid over a wide range of northern temperate and boreal species and leaf physiognomies. The results provide the basis to characterize important variability between and within species, and across ecological gradients using a rapid, cost-effective, easily replicated method.},
  language = {en},
  timestamp = {2016-03-31T17:56:18Z},
  number = {7},
  urldate = {2016-03-31},
  journal = {Ecological Applications},
  author = {Serbin, Shawn P. and Singh, Aditya and McNeil, Brenden E. and Kingdon, Clayton C. and Townsend, Philip A.},
  month = oct,
  year = {2014},
  keywords = {foliar chemistry,forests,Partial least squares regression (PLSR),plant functional traits,reflectance spectroscopy,Remote sensing,unread},
  pages = {1651--1669},
  file = {Serbin et al_2014_Spectroscopic.pdf:/home/ashiklom/MEGAsync/Zotero/Serbin et al_2014_Spectroscopic.pdf:application/pdf},
