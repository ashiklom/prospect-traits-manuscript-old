  urldate = {2017-01-09},
  journal = {Ecology and Evolution},
  author = {D{\'\i}az, Sandra and Purvis, Andy and Cornelissen, Johannes H. C. and Mace, Georgina M. and Donoghue, Michael J. and Ewers, Robert M. and Jordano, Pedro and Pearse, William D.},
  month = sep,
  year = {2013},
  keywords = {Biodiversity loss,ecosystem vulnerability,functional traits,Notes,Notes: Skimmed,phylogenetic conservatism,plant functional traits,specific effect function,specific response function},
  pages = {2958--2975},
  file = {Diaz et al_2013_Functional traits,.pdf:/home/ashiklom/MEGAsync/Zotero/Diaz et al_2013_Functional traits,.pdf:application/pdf},
