@article{powers_evaluating_2016,
  title = {Evaluating {{Natural Reserve Design Efficacy}} in the {{Canadian Boreal Forest Using Time Series AVHRR Data}}},
  volume = {0},
  issn = {0703-8992},
  doi = {10.1080/07038992.2016.1171065},
  abstract = {This research evaluates the efficacy of candidate reserves in boreal ecosystems with respect to a long term record of remote sensing derived productivity based on the dynamic habitat index (DHI) generated using Advanced Very High Resolution Radiometer (AVHRR) data (1987\textendash{}2007) and compared differences related to reserve location (stratified by land cover, ecozone and gross primary productivity (GPP)) and reserve size. Effectiveness of candidate reserves was assessed by how productivity values differed from the initial conditions (2000\textendash{}2005 baseline). Results indicate that small reserves ($<$1000 km2) at high elevations, high latitudes, intermittent environments (wetlands) or dominated by open shrub experienced the greatest amount of inter-annual variability. Alternately, larger reserves ($\geq$1000 km2; $<$10000 km2) were stable under these same conditions. Results also indicate that reserves located in highly productive areas ($>$700 kgC m-2 yr-1) experienced greater inter-annual variability than low productivity areas. This approach provides an objective and consistent means of evaluating reserve efficacy across different geographic areas and through time. By highlighting uncertainty associated with change impacts, this approach also offers opportunities to develop more robust long-term conservation targets in new reserves and to test potential mitigation strategies prior to implementation.R{\'e}sum{\'e}. Cette recherche {\'e}value l'efficacit{\'e} de r{\'e}serves potentielles dans les {\'e}cosyst{\`e}mes bor{\'e}aux en lien avec des mesures {\`a} long terme de productivit{\'e} obtenue par t{\'e}l{\'e}d{\'e}tection et sur la base de l'indice dynamique d'habitat (IDH) g{\'e}n{\'e}r{\'e} {\`a} l'aide des donn{\'e}es du radiom{\`e}tre avanc{\'e} {\`a} tr{\`e}s haute r{\'e}solution (AVHRR) (1987\textendash{}2007) et par rapport aux diff{\'e}rences li{\'e}es {\`a} l'emplacement de la r{\'e}serve (stratifi{\'e} par la couverture terrestre, l${'}${\'e}cozone, la productivit{\'e} primaire brute (PPB) et la taille de la r{\'e}serve.) L'efficacit{\'e} de r{\'e}serves potentielles a {\'e}t{\'e} {\'e}valu{\'e}e par la fa{\c c}on dont les valeurs de productivit{\'e} diff{\`e}rent des conditions initiales (2000\textendash{}2005 de r{\'e}f{\'e}rence). Les r{\'e}sultats indiquent que les petites r{\'e}serves ($<$1000 km2) {\`a} haute altitude, {\`a} hautes latitudes, les milieux intermittents (zones humides) ou les environnements domin{\'e}s par de la v{\'e}g{\'e}tation arbustive ouverte ont connu la plus grande variabilit{\'e} interannuelle. En revanche, les grandes r{\'e}serves ($\geq$1000 km2; $<$10000 km2) {\'e}taient stables dans ces m{\^e}mes conditions. Les r{\'e}sultats indiquent {\'e}galement que les r{\'e}serves situ{\'e}es dans des zones tr{\`e}s productives ($>$700 kgC m-2 yr-1) ont connu une plus grande variabilit{\'e} interannuelle que les zones de faible productivit{\'e}. Cette approche fournit un moyen objectif et coh{\'e}rent d'{\'e}valuer l'efficacit{\'e} des r{\'e}serves {\`a} travers les diff{\'e}rentes zones g{\'e}ographiques et dans le temps. En mettant en {\'e}vidence l'incertitude associ{\'e}e aux impacts du changement, cette approche offre {\'e}galement la possibilit{\'e} de d{\'e}velopper des objectifs de conservation {\`a} long terme plus robustes dans de nouvelles r{\'e}serves et de tester des strat{\'e}gies d'att{\'e}nuation possibles avant leur mise en {\oe}uvre.},
  timestamp = {2016-05-13T14:01:00Z},
  number = {ja},
