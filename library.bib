  number = {3},
  urldate = {2016-05-13},
  journal = {Remote Sensing},
  author = {McManus, Kelly M. and Asner, Gregory P. and Martin, Roberta E. and Dexter, Kyle G. and Kress, W. John and Field, Christopher B.},
  month = feb,
