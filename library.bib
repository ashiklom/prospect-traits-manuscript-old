@article{hellmann_isoscapes_2016,
  title = {Isoscapes Resolve Species-Specific Spatial Patterns in Plant\textendash{}plant Interactions in an Invaded {{Mediterranean}} Dune Ecosystem},
  issn = {0829-318X, 1758-4469},
  doi = {10.1093/treephys/tpw075},
  abstract = {Environmental heterogeneity and plant\textendash{}plant interactions are key factors shaping plant communities. However, the spatial dimension of plant\textendash{}plant interactions has seldom been addressed in field studies. This is at least partially rooted in a lack of methods that can accurately resolve functional processes in a spatially explicit manner. Isoscapes, that is, spatially explicit representations of stable isotope data, provide a versatile means to trace functional changes on spatial scales, for example, related to N-cycling (foliar $\delta$15N) and water use efficiency (WUEi, foliar $\delta$13C). In a case study in a nutrient-depleted Mediterranean dune ecosystem, we analysed the spatial impact of the invasive N2-fixing Acacia longifolia on three native species of different functional types using $\delta$15N and $\delta$13C isoscapes and spatial autocorrelation analyses. Isoscapes revealed strong spatial patterns in $\delta$15N and $\delta$13C with pronounced species-specific differences, demonstrating distinct spatial ranges of plant\textendash{}plant interactions. A coniferous tree and an ericaceous dwarf shrub showed significant enrichment in $\delta$15N within a range of 5\textendash{}8 m surrounding the canopy of A. longifolia, indicating input of N originating from symbiotic N2-fixation by the invader. In the dwarf shrub, which was most responsive to invader influence, enrichment in $\delta$13C additionally demonstrated spatially explicit changes to WUEi, while a native N2-fixer was unresponsive to the presence of the invader. Furthermore, $\delta$15N and $\delta$13C isoscapes yielded different patterns, indicating that plant\textendash{}plant interactions can have distinct spatial distributions and ranges based on the process measured. Additionally, the magnitude of the effect differed between field situations with high and low invasion pressure. This study highlights that the spatial scale must be accounted for when assessing the effects and outcome of species interactions. Functional tracers such as stable isotopes enable us to quantify spatial ranges of plant\textendash{}plant interactions, providing empirical data that can help to better understand and predict complex species interactions in multifaceted natural environments.},
  language = {en},
  timestamp = {2016-09-06T19:45:27Z},
  urldate = {2016-09-06},
  journal = {Tree Physiology},
  author = {Hellmann, Christine and Rascher, Katherine G. and Oldeland, Jens and Werner, Christiane},
  month = sep,
  year = {2016},
  keywords = {Biogeochemistry,Biome: Mediterranean,competition,facilitation,isotopes,N-fixing tree,nitrogen fixation,Notes,Notes: Skimmed,woody plant invasion,δ13C,δ15N},
  groups = {NP traits,PROSPECT traits,Landsat inversion,NP traits,PROSPECT traits,Landsat inversion},
  pmid = {27587484}
}

@incollection{goldstein_facing_2016,
  series = {Tree Physiology},
  title = {Facing {{Shortage}} or {{Excessive Light}}: {{How Tropical}} and {{Subtropical Trees Adjust Their Photosynthetic Behavior}} and {{Life History Traits}} to a {{Dynamic Forest Environment}}},
