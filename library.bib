  author = {Cai, Huiying and Di, Xueying and Chang, Scott X. and Jin, Guangze},
  month = apr,
  year = {2016},
  keywords = {Biodiversity,Biodiversity-ecosystem function relationship,canopy structure,Dominant species,Ecology,Forestry,Notes,Notes: Skimmed,Old-growth forest,Plant Sciences,Secondary forest,species richness,Stand structure,succession},
  pages = {1--9},
