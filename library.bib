  month = dec,
  year = {2015},
  keywords = {chlorophyll,Chlorophyll concentration,High spatial resolution imagery,Notes,Notes: Skimmed,precision agriculture,Relevance vector machine,Remote sensing,RS: UAV,Thermal remote sensing},
  pages = {32--42},
  file = {Elarab et al_2015_Estimating.pdf:/home/ashiklom/MEGAsync/Zotero/Elarab et al_2015_Estimating.pdf:application/pdf}
