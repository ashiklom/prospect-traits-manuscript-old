  author = {Caughlin, T. Trevor and Graves, Sarah J. and Asner, Gregory P. and {van Breugel}, Michiel and Hall, Jefferson S. and Martin, Roberta E. and Ashton, Mark S. and Bohlman, Stephanie A.},
  month = dec,
  year = {2016},
  keywords = {Biome: Tropical,canopy biology,field planting trials,forest dynamics,Hyperspectral,LiDaR: Airborne,Light detection and ranging,Notes,Notes: Skimmed,Panama,plantation,plant functional traits,precision forestry,reforestation,Remote sensing,RS: Imaging spectroscopy,RS: LiDAR,tree demography,Tropical forest},
  pages = {2369--2375},
  file = {Caughlin et al_2016_A hyperspectral.pdf:/home/ashiklom/MEGAsync/Zotero/Caughlin et al_2016_A hyperspectral.pdf:application/pdf},
