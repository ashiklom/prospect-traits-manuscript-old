  journal = {Journal of Geophysical Research: Biogeosciences},
  author = {Lowman, Lauren E. L. and Barros, Ana P.},
  month = apr,
  year = {2016},
  keywords = {0414 Biogeochemical cycles; processes; and modeling,0426 Biosphere/atmosphere interactions,0428 Carbon cycling,carbon cycle,GPP,Soil moisture,Tropical cyclones,unread,Water uptake},
  pages = {2015JG003279},
  file = {Lowman_Barros_2016_Interplay of.pdf:/home/ashiklom/MEGAsync/Zotero/Lowman_Barros_2016_Interplay of.pdf:application/pdf}
