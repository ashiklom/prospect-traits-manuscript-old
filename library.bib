  month = apr,
  year = {2015},
  keywords = {0476 Plant ecology,Biosphere/atmosphere interactions,carbon cycle,carbon cycle,chlorophyll,chlorophyll fluorescence,Diel; seasonal; and annual cycles,gross primary production,Leaf pigments,plant physiology,Remote sensing,spectroscopy,Sun Induced Fluorescence (SIF),unread},
  pages = {2015GL063201},
