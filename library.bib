  number = {4},
  urldate = {2016-06-29},
  journal = {Global Change Biology},
  author = {Bronson, Dustin R. and Gower, Stith T. and Tanner, Myron and Linder, Sune and Van Herk, Ingrid},
  month = apr,
  year = {2008},
  keywords = {Biome: Boreal,black spruce,carbon cycle,climate change,Experiment,Experiment: Warming,global warming,Picea mariana,Q10,soil CO2 flux,Soil respiration,soil temperature acclimation},
  pages = {856--867},
