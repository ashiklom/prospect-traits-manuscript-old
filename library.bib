  keywords = {Irradiance,leaf life span,Leaf mass per area,Nitrogen,photosynthesis,plant functional traits,plant strategies,rainfall,temperature,unread},
  pages = {411--421},
  file = {Wright et al_2005_Modulation of leaf.pdf:/home/ashiklom/MEGAsync/Zotero/Wright et al_2005_Modulation of leaf.pdf:application/pdf},
