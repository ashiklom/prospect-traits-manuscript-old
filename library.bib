  keywords = {bird diversity,Black-throated Blue Warbler,canopy structure,canopy structure,Dendroica caerulescens,habitat quality,Hubbard Brook Experimental Forest,LiDAR,LiDaR: Airborne,Neotropical migratory birds,New Hampshire,northern hardwoods forests,Remote sensing,unread,USA,waveform lidar data},
  pages = {1569--1576},
