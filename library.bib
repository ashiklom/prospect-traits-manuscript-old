  author = {Zarco-Tejada, P. J. and Su{\'a}rez, L. and Gonz{\'a}lez-Dugo, V.},
  month = jul,
  year = {2013},
  keywords = {canopy heterogeneity,canopy modeling,chlorophyll fluorescence retrieval,Fluorescence,FluorFLIM model,FluorMODleaf model,FluorSAIL model,Fraunhofer line depth (FLD),Fraunhofer line depth method,geometric forest light interaction model model,geophysical signal processing,global vegetation monitoring,heterogeneous,heterogeneous canopy,high resolution airborne hyperspectral imagery,Image resolution,low resolution global fluorescence map,Notes,Notes: Skimmed,physiology,Radiative transfer,radiative transfer simulation,RS: High-resolution/Commercial,RS: Imaging spectroscopy,signal retrieval,Spatial resolution,spatial resolution effect,tree crown,unmanned aerial vehicle (UAV),vegetation mapping},
  pages = {937--941},
  groups = {Landsat inversion,EDR,Landsat inversion,EDR}
