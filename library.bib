  author = {Messier, Julie and McGill, Brian J. and Lechowicz, Martin J.},
  month = jul,
  year = {2010},
  keywords = {Biome: Tropical,Ecological scaling,environmental filtering,foliar traits,habitat filtering,leaf dry matter content,leaf functional traits,leaf mass area,leaf mass per unit area (LMA),Location: Panama,plant community assembly,plant functional traits,trait-based ecology,tropical rainforests,variance components},
  pages = {838--848},
