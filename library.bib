@article{shipley_functional_2005,
  title = {Functional Linkages between Leaf Traits and Net Photosynthetic Rate: Reconciling Empirical and Mechanistic Models},
  volume = {19},
  issn = {1365-2435},
  shorttitle = {Functional Linkages between Leaf Traits and Net Photosynthetic Rate},
  doi = {10.1111/j.1365-2435.2005.01008.x},
  abstract = {* 1We had two objectives: (i) to determine the generality of, and extend the applicability of, a previously reported empirical relationship between leaf-level net photosynthetic rate (AM, nmol~g-1~s-1), specific leaf area (SLA, m2~kg-1) and leaf nitrogen mass fraction (NM, mmol~g-1); and (ii) to compare these empirical results with a mechanistic model of photosynthesis in order to provide a mechanistic justification for the empirical pattern.
* 2Our results were based on both literature and original data. There were a total of 160 and 87 data points for the leaf-level and whole-plant data, respectively.
* 3Our multiple regression for single leaves was ln(AM)~=~0$\cdot$66~+~0$\cdot$71~ln(SLA)~+ 0$\cdot$79~ln(NM), r2~=~-0$\cdot$80; only the intercept (0$\cdot$11) differed for the whole-plant data. These results are not significantly different from previously published relationships.
* 4We then converted the mechanistic model of Evans and Poorter, and a modified version which includes leaf lamina thickness (T) and leaf dry matter (tissue) concentration (CM), into directed acyclic graphs. We then derived reduced graphs that involved only T, CM, SLA, NM and AM. These were tested using structural equation modelling, with measured lamina thickness (T${'}$) and leaf dry matter ratio (LDMR, g dry mass g-1 fresh mass) as indicators of T and CM. The original Evans\textendash{}Poorter model was rejected, but the modified version fitted the structural relationships well. The same qualitative models also applied to the whole-plant data, although the path coefficients sometimes differed.
* 5Using simulations, we show that the original Evans\textendash{}Poorter model predicts a positive correlation between SLA and NM that maximizes AM. The data closely follow this predicted relationship. The correlation between the actual values of AM (standardized units) and the predicted values obtained from the modified Evans\textendash{}Poorter model was 0$\cdot$74 and increased to 0$\cdot$82 once three outlier points were removed.
* 6These results provide a mechanistic explanation for the empirical trends relating leaf form and carbon fixation, and predict that SLA and leaf N must be quantitatively co-ordinated to maximize C fixation.},
  language = {en},
  timestamp = {2016-05-16T15:19:30Z},
  number = {4},
  urldate = {2016-05-16},
  journal = {Functional Ecology},
  author = {Shipley, B. and Vile, D. and Garnier, E. and Wright, I. J. and Poorter, H.},
  month = aug,
  year = {2005},
  keywords = {assimilation rate,foliar traits,LDMR,leaf mass per unit area (LMA),leaf nitrogen,leaf nitrogen mass fraction,leaf thickness,leaf tissue concentration,leaf tissue density,Notes,Notes: Skimmed,path analysis,photosynthesis,plant functional traits,SLA,structural equation models,unread},
  pages = {602--615},
